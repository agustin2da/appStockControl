<div class="container">
  <table class="table table-striped-columns">
    <thead>
      <tr>
        <th scope="col">Fecha</th>
        <th scope="col">Codigo</th>
        <th scope="col">Nombre</th>
        <th scope="col">Descripcion</th>
        <th scope="col">Categoria</th>
        <th scope="col">Operario</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
        <tr>
          <td>{{date}}</td>
          <td>{{itemCode}}</td>
          <td>{{itemName}}</td>
          <td>{{description}}</td>
          <td>{{itemCategory}}</td>
          <td>{{user}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>